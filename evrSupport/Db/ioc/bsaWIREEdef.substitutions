#==============================================================================
#
# Abs:  Beam Synchronous Acquisition records for a single 
#	wire.
#
# Name: bsaWIREEdef.substitutions
#
# Note: Must follow the LCLS naming conventions.  
#
#==============================================================================
#
file bsaATTREdef.db
{
#           Device   Attribute  Input      EGU   HOPR  LOPR PREC ADEL   Link
  pattern {   D       ,ATTR,    I         , EG ,  HO  , LO , PR , AD  ,  LNK,       SINK_SIZE   }
	  { $(DEVICE) ,POSN,$(DEVICE):POSN, um ,  10  ,  0 ,  3 , 0.1 , "",        $(SINK_SIZE\=10) }
	  { $(DEVICE) ,MASK,$(DEVICE):MASK, um  ,65535,  0 ,  3 , 1   , $(SLCLNK), $(SINK_SIZE\=10) }
}
